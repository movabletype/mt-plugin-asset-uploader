{
  "name": "asset-uploader",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "copy:app": "copyfiles -f node_modules/@movabletype/app/dist/bundle/* app-dist/ && copyfiles -f node_modules/@movabletype/app/dist/bundle/assets/* app-dist/assets/",
    "dev": "npm run copy:app && vite build --watch",
    "build": "npm run copy:app && rimraf dist && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@movabletype/app": "file:../../../@movabletype/movabletype-app-0.0.2.tgz",
    "@movabletype/svelte-components": "file:../../../@movabletype/movabletype-svelte-components-0.0.4.tgz",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@types/eslint": "^9.6.1",
    "@types/jsdom": "^21.1.7",
    "copyfiles": "^2.4.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-svelte": "^3.5.1",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "rimraf": "^5.0.10",
    "svelte": "^5.28.2",
    "svelte-check": "^4.1.7",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
