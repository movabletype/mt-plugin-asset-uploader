{
  "name": "asset-uploader",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "copy:app": "cpx 'node_modules/@movabletype/app/dist/bundle/**/*' app-dist",
    "dev": "npm run copy:app && vite build --watch",
    "build": "npm run copy:app && rimraf dist && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@movabletype/app": "../../../app.tgz",
    "@movabletype/svelte-components": "../../../svelte-components.tgz",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@types/eslint": "^8.56.7",
    "cpx": "^1.5.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.1.2",
    "rimraf": "^5.0.7",
    "svelte": "^5.0.0-next.1",
    "svelte-check": "^3.6.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0-alpha.20",
    "vite": "^5.0.11",
    "vitest": "^1.2.0"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
