{
  "name": "@movabletype/app",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build:esm": "vite build --outDir dist/esm && rimraf ../../../../mt-static/plugins/AssetUploader/app-dist && cpx 'dist/esm/**/*' ../../../../mt-static/plugins/AssetUploader/app-dist",
    "build:cjs": "cross-env OUTPUT_FORMAT=cjs vite build --outDir dist/cjs",
    "build:type": "tsc --emitDeclarationOnly --declaration --declarationMap --outDir dist/types",
    "build": "rimraf dist && npm run build:esm && npm run build:cjs && npm run build:type",
    "test": "tsc --noEmit && echo \"Error: no test specified\" && exit 1",
    "lint": "prettier --check . && eslint .",
    "format:fix": "prettier --write --cache --cache-strategy metadata ."
  },
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./ui": {
      "types": "./dist/types/ui.d.ts",
      "import": "./dist/esm/ui.js",
      "require": "./dist/cjs/ui.js"
    }
  },
  "typesVersions": {
    "*": {
      "ui": [
        "./dist/types/ui.d.ts"
      ]
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jquery": "^3.5.30",
    "@types/node": "^20.13.0",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.39.0",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
    "rimraf": "^5.0.7",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.0.0-alpha.20",
    "vite": "^5.2.12",
    "vitest": "^1.6.0"
  }
}
